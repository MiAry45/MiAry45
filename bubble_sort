#include <iostream>
using namespace std;
int bubble_sort(int arr[]){

int n =8;
for(int i= n-1;i>=0;i--){
   for(int j = 0;j<= i-1;j++){
  if(arr[j] > arr[j+1])
         swap(arr[j], arr[j+1]);
   }
}

for(int i =0;i<n;i++){
cout<<arr[i]<<" ";
}
}
int main(){
   int arr[] = {5,3,1,6,9,0,1,2};
   bubble_sort(arr);
}
