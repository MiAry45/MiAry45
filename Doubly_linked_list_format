class Node{
    public:
    int data;
    Node *next;
    Node *back;
    public:
    // contructor self referential contructor
    Node(int data1,Node* next1,Node*back1){
        data = data1;
        next = next1;
        back = back1;
    }
    Node(int data1){
        data = data1;
        next = nullptr;
        back = nullptr;
    }
    Node(int data1,Node* next1){
        data = data1;
        next = next1;
        back = nullptr;
    }
    Node(int data1,Node*back1){
        data = data1;
        next = nullptr;
        back = back1;
    }
};
